{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    ["typecheck", { "disable": { "production": true } }]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": [
        ["webpack-loaders", { "config": "webpack.config.node.js", "verbose": false }]
      ]
    },
    "production": [
      "transform-decorators-legacy",
      "add-module-exports",
      "react-remove-prop-types"
    ]
  }
}
